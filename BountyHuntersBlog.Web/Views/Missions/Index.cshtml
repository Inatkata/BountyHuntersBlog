@using Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<BountyHuntersBlog.ViewModels.MissionViewModel>
<h1>Missions</h1>
<table class="table">
    <thead><tr><th>Title</th><th>Author</th><th></th></tr></thead>
    <tbody>
    @foreach(var m in Model) {
        <tr>
            <td>@m.Title</td>
            <td>@m.AuthorName</td>
            <td><a asp-action="Details" asp-route-id="@m.Id">Details</a></td>
        </tr>
    }
    </tbody>
</table>